	"version": "0.2.0",
	"configurations": [
		{
			"name": "attach client",
			"type": "cppdbg",
			"request": "attach",
			"program": "${workspaceFolder}/client",
			"processId": "${command:pickProcess}",
			"MIMode": "lldb"
		},
		{
			"name": "launch server",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/server",
			"args": [
				"4242"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "lldb"
		},
		{
			"name": "launch client",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/client",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "lldb"
		}
	]
}
